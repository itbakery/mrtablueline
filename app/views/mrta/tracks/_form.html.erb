<%= form_for [:mrta,@track], :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :project_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :project_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :atmonth, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :atmonth, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :projection, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :projection, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :actual, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :actual, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :reportnote, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :reportnote, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                mrta_tracks_path, :class => 'btn' %>
  </div>
<% end %>
