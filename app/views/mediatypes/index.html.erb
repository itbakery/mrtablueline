<h1>Listing mediatypes</h1>

<table>
  <tr>
    <th>Name th</th>
    <th>Name en</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mediatypes.each do |mediatype| %>
  <tr>
    <td><%= mediatype.name_th %></td>
    <td><%= mediatype.name_en %></td>
    <td><%= link_to 'Show', mediatype %></td>
    <td><%= link_to 'Edit', edit_mediatype_path(mediatype) %></td>
    <td><%= link_to 'Destroy', mediatype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

